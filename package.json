  "license": "MIT",
  "bugs": {
    "url": "https://github.com/realmjs/account-client/issues"
  },
  "homepage": "https://github.com/realmjs/account-client#readme",
  "dependencies": {
    "@realmjs/xhttp-request": "^1.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  }
