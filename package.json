{
  "name": "@realmjs/account-client",
  "version": "1.2.1",
  "description": "Client side scripts to authenticate with @realmjs/account-restapi",
  "main": "dist/account.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "babel src --out-dir dist --source-maps",
    "prepare": "npm run clean & npm run build",
    "start": "webpack-dev-server --config webpack.dev.config.js  --port 3200 --host 0.0.0.0 --watch-poll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/realmjs/account-client.git"
  },
